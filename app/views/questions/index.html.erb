<div id="main_title">Answers for Incoming Boots</div>
  <div id="homepage_title_container">
    <% @questions.each do |question| %>
      <div class="homepage_titles">
        <% if current_user %>
          <%= render partial: 'shared/question_voting', locals: {voteable: question} %>
        <% end %>
        <div class="question">
        <%= link_to question.title, question_path(question)%>
        </div>
      </div>
      <% end %>
  </div>
  <div class="images">
    <img src="http://devbootcamp.com/imgs/get-involved-staff.png">
  </div>



